<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="507.0" prefWidth="617.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EssaiController">
   <MenuBar layoutY="2.0" prefHeight="26.0" prefWidth="736.0">
	 <menus>
	   <Menu mnemonicParsing="false" text="File">
		 <items>
		   <MenuItem mnemonicParsing="false" text="Open file" />
			   <MenuItem mnemonicParsing="false" text="Create file" />
		 </items>
	   </Menu>
	   <Menu mnemonicParsing="false" text="Edit">
		 <items>
		   <MenuItem mnemonicParsing="false" text="Delete" />
		 </items>
	   </Menu>
		<Menu mnemonicParsing="false" text="Help">
			<items>
				<MenuItem mnemonicParsing="false" text="doc">
					<graphic>
						<Hyperlink fx:id="siteWebLink" contentDisplay="RIGHT" text="Documentation" />
					</graphic>
				</MenuItem>
				<MenuItem mnemonicParsing="false" text="About" />
			</items>
		</Menu>
	</menus>
	</MenuBar>

	<ScrollPane layoutX="25.0" layoutY="34.0" prefHeight="387.0" prefWidth="413.0">
		<content>
			<TableView fx:id="tableView" editable="true" prefHeight="387.0" prefWidth="413.0" />
		</content>
	</ScrollPane>

	<TextField fx:id="txtFld" layoutX="479.0" layoutY="157.0" prefHeight="58.0" prefWidth="98.0" />
	<Button layoutX="490.0" layoutY="66.0" onAction="#ajouter" prefHeight="30.0" prefWidth="76.0" text="Ajouter" />
	<Button layoutX="490.0" layoutY="110.0" onAction="#supprimer" text="Supprimer" />

</AnchorPane>
